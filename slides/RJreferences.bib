	file = {Md Ghani et al. - 2010 - Analysis of geometric moments as features for fire.pdf:C\:\\Users\\jzemm\\Zotero\\storage\\AJYTZ6MS\\Md Ghani et al. - 2010 - Analysis of geometric moments as features for fire.pdf:application/pdf}
}

